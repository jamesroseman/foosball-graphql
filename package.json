    "@types/react-router": "^4.0.11",
    "body-parser": "^1.15.2",
    "chalk": "^1.1.3",
    "debug": "^2.2.0",
    "express": "^4.14.0",
    "express-graphql": "^0.6.6",
    "graphql": "^0.10.1",
    "ip": "^1.1.5",
    "morgan": "^1.7.0",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-router": "^4.1.1"
  },
